        <ul class="c-step_list">
          <li class="c-step_list_item c-step_list_item-01">
            <div class="c-step_list_item_img">
              <%- include(relativePath + '_utilities/_picture', {src: '/img/sell/gif/web_01_sm.gif', alt: 'イメージ画像：PC・スマホでお申込み'});%>
            </div>
            <div class="c-step_list_item_body">
              <h4><span class="RalewayBold">STEP</span>01</h4>
              <p>PC・スマホで<br class="u-hidden-md u-hidden-lg">お申込み</p>
            </div>
          </li>
          <li class="c-step_list_item c-step_list_item-02">
            <div class="c-step_list_item_img">
              <%- include(relativePath + '_utilities/_picture', {src: '/img/sell/gif/web_02_sm.gif', alt: 'イメージ画像：商品を指定日時に集荷'});%>
            </div>
            <div class="c-step_list_item_body">
              <h4><span class="RalewayBold">STEP</span>02</h4>
              <p>商品を指定日時に<br class="u-hidden-md u-hidden-lg">集荷</p>
            </div>
          </li>
          <li class="c-step_list_item c-step_list_item-03">
            <div class="c-step_list_item_img">
              <%- include(relativePath + '_utilities/_picture', {src: '/img/sell/gif/web_03_sm.gif', alt: 'イメージ画像：バイヤーが査定し入金'});%>
